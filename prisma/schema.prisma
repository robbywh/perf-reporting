generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Sprint {
  id         String   @id @db.VarChar(36) 
  name       String   @db.VarChar(36) 
  start_date DateTime                   
  end_date   DateTime                    

  @@map("sprint")
}